{
  "name": "react-boilerplate",
  "version": "4.0.1",
  "description": "A slightly opinionated yet dead simple boilerplate for ReactJS",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-prod-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "build:gh-pages": "cross-env NODE_ENV=gh-pages webpack --progress --colors",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "fix": "eslint --fix src/**/*.js src/**/*.jsx"
  },
  "author": "Mike Chabot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/react-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mikechabot/react-boilerplate/issues"
  },
  "browserslist": [
    "defaults"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "temp-directory": "./.nyc-coverage-temp",
    "report-dir": "./.nyc-coverage"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bulma": "^0.9.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-tabify": "0.1.25",
    "redux": "^4.0.5",
    "redux-entity": "^8.0.14",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "css-loader": "^4.0.0",
    "del": "^5.1.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.3.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.12.21",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^9.0.2",
    "sinon": "^9.0.2",
    "style-loader": "^0.23.1",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^3.0.8",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
